import math
a=int(input("enter a value : "))
b=int(input("enter a value : "))
x1=(math.pow(a,2)+math.log(b))/(math.sin(a)+math.cos(b))
x2=(math.exp(a)+math.log10(b))/(math.pow(b,3)+math.tan(a))
x3=(math.factorial(a)+math.gcd(a,b))/(math.pow(a,b)+math.sqrt(math.pow(b,a)))
print("x1 = ",x1)
print("x2 = ",x2)
print("x3 = ",x3)
