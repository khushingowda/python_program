set1={1,0,9,8,2,4,5,7,6}
set2={1,0,9,6,3,2,7,5}
print("union of set1 and set2 = ",set1.union(set2))
print("intersection of set1 and set2 = ",set1.intersection(set2))
print("symmeteric difference of set1 and set2 = ",set1.symmetric_difference(set2))
set3={1,0,9}
if set3.issubset(set1) and set3.issubset(set2):
    print("{1,0,9} is a subset of set1 and set2")
else:
    print("not a subset")
print("difference of set1 and set2 is = ",set1.difference(set2))
